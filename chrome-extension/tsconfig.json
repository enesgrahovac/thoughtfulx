{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "lib": ["dom", "es6"],
    "allowJs": true,
    "jsx": "react",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "types": ["chrome"]
  },
  "include": ["src"]
}
